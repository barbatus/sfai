{
  "name": "services",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./edge": {
      "import": "./dist/auth-edge.js",
      "types": "./dist/auth-edge.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
