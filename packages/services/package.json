    "lint": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "ts-rest": "workspace:*",
    "inversify": "^6.0.3",
    "jose": "^5.9.6",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^22",
    "eslint": "^8.56.0",
    "eslint-config": "workspace:*"
  }
}